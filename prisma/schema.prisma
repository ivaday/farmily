generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite" // or postgresql
  url      = "file:./dev.db"
}

model User {
  id        String   @id @default(cuid())
  name      String
  email     String   @unique
  password  String
  role      String
  createdAt DateTime @default(now())
}
